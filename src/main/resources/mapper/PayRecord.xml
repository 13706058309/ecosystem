<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cykj.mapper.PayRecordMapper">
    <insert id="addPayRecord" parameterType="com.cykj.entity.PayRecord">
        insert into payRecord (score, compID, detail, state) VALUES
        (#{score},#{compId},#{detail},#{state})
    </insert>
</mapper>
